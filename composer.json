{
  "name": "aivchen/yii2-simple-cache-adapter",
  "description": "PSR-16 Yii2 Adapter",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Devon Liu",
      "email": "devonliu02@gmail.com"
    },
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    },
    {
      "name": "Andrew Ivchenkov",
      "email": "reclamme@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "psr/simple-cache": "^3.0",
    "yiisoft/yii2": "^2.0.49"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28",
    "friendsofphp/php-cs-fixer": "^3.38",
    "phpunit/phpunit": "^10.4",
    "psalm/plugin-symfony": "^5.1",
    "vimeo/psalm": "^5.15",
    "yidas/yii2-composer-bower-skip": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Aivchen\\SimpleCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aivchen\\SimpleCache\\Tests\\": "tests"
    },
    "files": [
      "vendor/yiisoft/yii2/Yii.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "yiisoft/yii2-composer": false
    },
    "process-timeout": 1800,
    "sort-packages": true
  },
  "scripts": {
    "checkcs": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
    "fixcs": "./vendor/bin/php-cs-fixer fix",
    "psalm": "./vendor/bin/psalm --no-diff",
    "test": "./vendor/bin/phpunit"
  }
}
