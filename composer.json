{
  "name": "wearesho-team/yii2-simple-cache-adapter",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Devon Liu",
      "email": "devonliu02@gmail.com"
    },
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wearesho\\SimpleCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\SimpleCache\\Tests\\": "tests"
    },
    "files": [
      "vendor/yiisoft/yii2/Yii.php"
    ]
  },
  "require": {
    "php": ">=7.4",
    "psr/simple-cache": "^1.0",
    "yiisoft/yii2": "^2.0.35"
  },
  "require-dev": {
    "cache/integration-tests": "^0.16.0",
    "phpunit/phpunit": "^9.2",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
